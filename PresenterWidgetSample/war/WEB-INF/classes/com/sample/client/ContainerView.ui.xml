<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">

<ui:UiBinder xmlns:ui='urn:ui:com.google.gwt.uibinder'
	xmlns:g='urn:import:com.google.gwt.user.client.ui'
	ui:generateFormat='com.google.gwt.i18n.rebind.format.PropertiesFormat'
	ui:generateKeys='com.google.gwt.i18n.rebind.keygen.MD5KeyGenerator'
	ui:generateLocales='default'>

	<ui:style field='footer' src="ContentFooter.css" />
		
	<ui:style field='header' src="ContentHeader.css" />
		
	<ui:style field='menu' src="ContentMenu.css" />
		
	<g:DockLayoutPanel width="100%" height="100%">
		<g:north size="50">
			<g:HTMLPanel height="50px" width="100%" ui:field="northPanel" styleName="{header.header}"/>
		</g:north>
		<g:south size="40.0">
			<g:HTMLPanel height="50px" width="100%" ui:field="southPanel" styleName="{footer.footer}"/>
		</g:south>
		<g:center size="40.0">
			<g:HTMLPanel width="100%" height="100%" ui:field="centerPanel"/>
		</g:center>
		<g:west size="200">
			<g:HTMLPanel width="200px" height="100%" ui:field="westPanel" styleName="{menu.menu}"/>
		</g:west>
		<g:east size="200">
			<g:HTMLPanel width="200px" height="100%" ui:field="eastPanel" styleName="{menu.menu}"/>
		</g:east>
	</g:DockLayoutPanel>
</ui:UiBinder>
