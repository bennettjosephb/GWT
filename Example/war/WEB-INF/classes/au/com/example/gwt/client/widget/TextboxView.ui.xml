<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">

<ui:UiBinder xmlns:ui='urn:ui:com.google.gwt.uibinder'
	xmlns:g='urn:import:com.google.gwt.user.client.ui' xmlns:w='urn:import:au.com.example.gwt.client.widget'
	ui:generateFormat='com.google.gwt.i18n.rebind.format.PropertiesFormat'
	ui:generateKeys='com.google.gwt.i18n.rebind.keygen.MD5KeyGenerator'
	ui:generateLocales='default'>

	<ui:style>
		.textBoxStyle {
			font-size: medium;
			padding-right: 10px;
			padding-left: 10px;
		}
		
		.textBoxStyle: focus {
			border: 4px solid #6d0019;
			-webkit-box-shadow: rgba(0, 0, 0, 0.298039) 0 1px 3px inset;
			box-shadow: rgba(0, 0, 0, 0.298039) 0 1px 3px inset;
			outline: none;
		}
	</ui:style>

	<g:HTMLPanel>

		<w:TextboxWithImage />
		<g:TextBox width="300px" height="30px" ui:field="textField" />
		<g:TextBox width="300px" height="30px" ui:field="result"/>
		<g:TextBox width="300px" height="30px" />
		<g:TextBox width="300px" height="30px" />
		<g:TextBox width="300px" height="30px" />
		<g:TextBox width="300px" height="30px" />
		<g:TextBox width="300px" height="30px" />
		<g:TextBox width="300px" height="30px" />
		<g:TextBox width="300px" height="30px" />
		<g:TextBox width="300px" height="30px" />
		<g:TextBox width="300px" height="30px" />
		<g:TextBox width="300px" height="30px" />
		<g:TextBox width="300px" height="30px" />
		<g:TextBox width="300px" height="30px" />
		Textbox Example
	</g:HTMLPanel>
</ui:UiBinder>
