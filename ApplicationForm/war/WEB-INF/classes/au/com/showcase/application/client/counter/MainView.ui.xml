<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">

<ui:UiBinder xmlns:ui='urn:ui:com.google.gwt.uibinder'
	xmlns:g='urn:import:com.google.gwt.user.client.ui' ui:generateFormat='com.google.gwt.i18n.rebind.format.PropertiesFormat'
	ui:generateKeys='com.google.gwt.i18n.rebind.keygen.MD5KeyGenerator'
	ui:generateLocales='default' xmlns:p1="urn:import:com.google.gwt.user.cellview.client">

	<g:HTMLPanel>
		<g:ListBox width="50px" ui:field="serialNoList" />
		<g:ListBox width="150px" ui:field="dateList" />
		<g:ListBox width="150px" ui:field="usernameList" />
		<g:ListBox width="150px" ui:field="counterRefNoList" />
		<g:ListBox width="150px" ui:field="transactionRefNoList" />
		<g:ListBox width="110px" ui:field="transactionTypeList" />
		<g:ListBox width="100px" ui:field="totalAmountList" />
		<g:ListBox width="100px" ui:field="pendingAmountList" />
		<g:ListBox width="110px" ui:field="balanceAmountList" />
		<g:ListBox width="110px" ui:field="collectionList" />
		<g:ListBox width="100px" ui:field="paymentList" />
		<p1:DataGrid width="100%" height="200px" ui:field="pendingTransaction" />
		<g:Button ui:field="button" text="Click This!!!" />
	</g:HTMLPanel>
</ui:UiBinder>
