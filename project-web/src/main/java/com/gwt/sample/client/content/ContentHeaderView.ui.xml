<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">

<ui:UiBinder xmlns:ui='urn:ui:com.google.gwt.uibinder'
	xmlns:g='urn:import:com.google.gwt.user.client.ui' ui:generateFormat='com.google.gwt.i18n.rebind.format.PropertiesFormat'
	ui:generateKeys='com.google.gwt.i18n.rebind.keygen.MD5KeyGenerator'
	ui:generateLocales='default'>
	<ui:style field='header' src="ContentHeader.css" />
    
	<ui:style field='common' src="../common/css/Common.css" />
    
	<g:HTMLPanel width="100%" height="71px"
		styleName="{header.loginscreen-header}">
		<g:HTMLPanel width="200px" styleName="{header.logo-padding}">
			<g:Image width="150px" height="40px" />
		</g:HTMLPanel>
		<g:HTMLPanel width="125px" styleName="{header.logout-button}">
			<g:Button width="75px" height="30px" styleName="{common.logoutButton}">Sign out</g:Button>
		</g:HTMLPanel>
	</g:HTMLPanel>
</ui:UiBinder>
