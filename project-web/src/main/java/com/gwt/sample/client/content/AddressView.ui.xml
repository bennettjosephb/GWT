<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">

<ui:UiBinder xmlns:ui='urn:ui:com.google.gwt.uibinder'
	xmlns:g='urn:import:com.google.gwt.user.client.ui' ui:generateFormat='com.google.gwt.i18n.rebind.format.PropertiesFormat'
	ui:generateKeys='com.google.gwt.i18n.rebind.keygen.MD5KeyGenerator'
	ui:generateLocales='default'>

	<ui:style field='content' src="Content.css" />
		
	<ui:style field='currentDetails' src="CurrentDetails.css" />
		
	<ui:style field='common' src="../common/css/Common.css" />
		
	<g:HTMLPanel width="100%" height="100%">
		<g:VerticalPanel width="500px" height="100%">
			<g:Cell width="100%" height="60px">
				<g:HTMLPanel styleName="{currentDetails.currentDetailsSpacing}">
					<g:Label>
						<ui:msg key="street1" description="Address Street1">Street1</ui:msg>
					</g:Label>
					<g:TextBox width="450px" styleName="{common.input}"
						ui:field="street1" maxLength="100"/>
					<g:Label ui:field="street1Error" />
				</g:HTMLPanel>
			</g:Cell>
			<g:Cell height="60px">
				<g:HTMLPanel styleName="{currentDetails.currentDetailsSpacing}">
					<g:Label>
						<ui:msg key="street2" description="Address Street2">Street2</ui:msg>
					</g:Label>
					<g:TextBox styleName="{common.input}" width="450px"
						ui:field="street2" />
					<g:Label ui:field="street2Error" />
				</g:HTMLPanel>
			</g:Cell>
			<g:Cell height="60px">
				<g:HTMLPanel styleName="{currentDetails.currentDetailsSpacing}">
					<g:Label>
						<ui:msg key="city" description="City/Town Name">City/Town</ui:msg>
					</g:Label>
					<g:TextBox styleName="{common.input}" width="450px"
						ui:field="city" />
					<g:Label ui:field="cityError" />
				</g:HTMLPanel>
			</g:Cell>
			<g:Cell height="60px">
				<g:HTMLPanel styleName="{currentDetails.currentDetailsSpacing}">
					<g:Label>
						<ui:msg key="state" description="State Name">State</ui:msg>
					</g:Label>
					<g:TextBox styleName="{common.input}" width="450px"
						ui:field="state" />
					<g:Label ui:field="stateError" />
				</g:HTMLPanel>
			</g:Cell>
			<g:Cell height="60px">
				<g:HTMLPanel styleName="{currentDetails.currentDetailsSpacing}">
					<g:Label>
						<ui:msg key="zipCode" description="Postal Zip Code">Postal Code</ui:msg>
					</g:Label>
					<g:TextBox styleName="{common.input}" width="450px"
						ui:field="zipCode" />
					<g:Label ui:field="zipCodeError" />
				</g:HTMLPanel>
			</g:Cell>
			<g:Cell height="">
				<g:HTMLPanel styleName="{currentDetails.currentDetailsSpacing}">
					<g:Label>
						<ui:msg key="country" description="Country Name">Country</ui:msg>
					</g:Label>
					<g:TextBox styleName="{common.input}" width="450px"
						ui:field="country" />
					<g:Label ui:field="countryError" />
				</g:HTMLPanel>
			</g:Cell>
		</g:VerticalPanel>
	</g:HTMLPanel>
</ui:UiBinder>
