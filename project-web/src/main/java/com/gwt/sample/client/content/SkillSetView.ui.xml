<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">

<ui:UiBinder xmlns:ui='urn:ui:com.google.gwt.uibinder'
	xmlns:g='urn:import:com.google.gwt.user.client.ui'
	ui:generateFormat='com.google.gwt.i18n.rebind.format.PropertiesFormat'
	ui:generateKeys='com.google.gwt.i18n.rebind.keygen.MD5KeyGenerator'
	ui:generateLocales='default'>

	<ui:style field='common' src="../common/css/Common.css" />
		
	<g:HTMLPanel width="100%" height="100%">
		<g:VerticalPanel spacing="10" width="100%" height="100%">
			<g:Cell width="100%" height="50px">
				<g:HTMLPanel height="50px"/>
			</g:Cell>
			<g:Cell width="100%" height="100%">
				<g:HTMLPanel width="100%" height="100%">
					<g:FlowPanel ui:field="listSkills"/>
				</g:HTMLPanel>
			</g:Cell>
			<g:HTMLPanel>
				<g:Button ui:field="addSkill" styleName="{common.button}">
					<ui:msg key="addSkill" description="Adding New Skill Set">Add Skill</ui:msg>
				</g:Button>
			</g:HTMLPanel>
		</g:VerticalPanel>
	</g:HTMLPanel>
</ui:UiBinder>