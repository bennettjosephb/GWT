<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">

<ui:UiBinder xmlns:ui='urn:ui:com.google.gwt.uibinder'
	xmlns:g='urn:import:com.google.gwt.user.client.ui' ui:generateFormat='com.google.gwt.i18n.rebind.format.PropertiesFormat'
	ui:generateKeys='com.google.gwt.i18n.rebind.keygen.MD5KeyGenerator'
	ui:generateLocales='default'>

	<ui:style field='footer' src="../../content/ContentFooter.css" />
		
	<ui:style field='header' src="../../content/ContentHeader.css" />
		
	<ui:style field='createAccount' src="CreateAccount.css" />
		
	<ui:style field='temp' src="Temp.css" />
		
	<ui:style>
		#footer {
			margin: -100px auto 0;
			min-width: 1000px;
			height: 100px;
			background: #BFF08E;
			position: relative;
		}
	</ui:style>

	<g:HTMLPanel>
		<g:DockPanel width="100%" >
			<g:Dock direction="NORTH">
				<g:HTMLPanel width="100%" height="50px" ui:field="headerPanel"
					styleName="{header.header}" />
			</g:Dock>
			<g:Dock direction="CENTER">
				<g:HTMLPanel width="1000px" height="100%" ui:field="contentPanel"
					styleName="{createAccount.centerAlighment}" />
			</g:Dock>
			<g:Dock direction="SOUTH">
				<g:HTMLPanel width="100%" height="50px" ui:field="footerPanel"/>
			</g:Dock>
		</g:DockPanel>
	</g:HTMLPanel>
</ui:UiBinder>
