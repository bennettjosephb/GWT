<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">

<ui:UiBinder xmlns:ui='urn:ui:com.google.gwt.uibinder'
	xmlns:g='urn:import:com.google.gwt.user.client.ui' ui:generateFormat='com.google.gwt.i18n.rebind.format.PropertiesFormat'
	ui:generateKeys='com.google.gwt.i18n.rebind.keygen.MD5KeyGenerator'
	ui:generateLocales='default'>

	<ui:style field='currentDetails' src="CurrentDetails.css" />
		
	<ui:style field='common' src="../common/css/Common.css" />
		
	<ui:style>
		.contentBGColor {
			background-color: #FFFFFF;
		}
	</ui:style>

	<g:HTMLPanel width="100%" height="100%" styleName="{style.contentBGColor}">
		<g:HorizontalPanel width="100%" height="100%">
			<g:VerticalPanel width="50%" height="100%" spacing="10">
				<g:Cell height="50px">
					<g:HTMLPanel height="50px" />
				</g:Cell>
				<g:Cell height="50px">
					<g:HTMLPanel width="100%" height=""
						styleName="{currentDetails.currentDetailsSpacing}">
						<g:Label text="New Label" />
						<g:SuggestBox width="450px" styleName="{common.input}"
							ui:field="currentDesignation" />
						<g:Label />
					</g:HTMLPanel>
				</g:Cell>
				<g:Cell height="50px">
					<g:HTMLPanel height=""
						styleName="{currentDetails.currentDetailsSpacing}" width="100%">
						<g:Label text="New Label" />
						<g:SuggestBox width="450px" styleName="{common.input}"
							ui:field="currentCompany" />
						<g:Label />
					</g:HTMLPanel>
				</g:Cell>
				<g:Cell height="50px">
					<g:HTMLPanel height=""
						styleName="{currentDetails.currentDetailsSpacing}" width="100%">
						<g:Label text="New Label" />
						<g:SuggestBox width="450px" styleName="{common.input}"
							ui:field="currentLocation" />
						<g:Label />
					</g:HTMLPanel>
				</g:Cell>
				<g:Cell height="50px">
					<g:HTMLPanel height=""
						styleName="{currentDetails.currentDetailsSpacing}" width="100%">
						<g:Label text="New Label" />
						<g:SuggestBox width="450px" styleName="{common.input}"
							ui:field="preferredLocation" />
						<g:Label />
					</g:HTMLPanel>
				</g:Cell>
				<g:Cell height="50px">
					<g:HTMLPanel height=""
						styleName="{currentDetails.currentDetailsSpacing}" width="100%">
						<g:Label text="New Label" />
						<g:SuggestBox width="450px" styleName="{common.input}"
							ui:field="functionalArea" />
						<g:Label />
					</g:HTMLPanel>
				</g:Cell>
				<g:Cell height="50px">
					<g:HTMLPanel height=""
						styleName="{currentDetails.currentDetailsSpacing}" width="100%">
						<g:Label text="New Label" />
						<g:SuggestBox width="450px" styleName="{common.input}"
							ui:field="role" />
						<g:Label />
					</g:HTMLPanel>
				</g:Cell>
				<g:Cell height="50px">
					<g:HTMLPanel height=""
						styleName="{currentDetails.currentDetailsSpacing}" width="100%">
						<g:Label text="New Label" />
						<g:ListBox width="450px" styleName="{common.select}"
							ui:field="category" />
						<g:Label />
					</g:HTMLPanel>
				</g:Cell>
				<g:Cell height="50px">
					<g:HTMLPanel height=""
						styleName="{currentDetails.currentDetailsSpacing}" width="100%">
						<g:Label text="New Label" />
						<g:ListBox width="450px" styleName="{common.select}"
							ui:field="subCategory" />
						<g:Label />
					</g:HTMLPanel>
				</g:Cell>
				<g:HTMLPanel height=""
					styleName="{currentDetails.currentDetailsSpacing}" width="100%">
					<g:Label />
					<g:SubmitButton styleName="{common.input}" text="Save" width="150px" ui:field="saveCurrentDetails" />
					<g:Label />
				</g:HTMLPanel>
			</g:VerticalPanel>
		</g:HorizontalPanel>
	</g:HTMLPanel>
</ui:UiBinder>
