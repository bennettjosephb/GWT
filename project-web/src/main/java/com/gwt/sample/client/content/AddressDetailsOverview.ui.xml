<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">
	<ui:style field='content' src="Content.css" />
		
	<ui:style field='currentDetails' src="CurrentDetails.css" />
		
	<ui:style field='common' src="../common/css/Common.css" />
		
	<g:HTMLPanel width="100%" height="100%">
		<g:VerticalPanel width="800px" height="100%">
			<g:Cell width="100%" height="30px">
				<g:HTMLPanel styleName="{currentDetails.currentDetailsSpacing}">
					<g:HorizontalPanel width="100%">
						<g:Cell width="150px" height="100%">
							<g:Label styleName="{content.contactDetailsLabel}">
								<ui:msg key="street1" description="Address Street1">Street1</ui:msg>
							</g:Label>
						</g:Cell>
						<g:Cell width="" height="100%">
							<g:Label ui:field="street1" height="100%" />
						</g:Cell>
					</g:HorizontalPanel>
				</g:HTMLPanel>
			</g:Cell>
			<g:Cell height="30px">
				<g:HTMLPanel styleName="{currentDetails.currentDetailsSpacing}">
					<g:HorizontalPanel width="100%">
						<g:Cell width="150px">
							<g:Label styleName="{content.contactDetailsLabel}">
								<ui:msg key="street2" description="Address Street2">Street2</ui:msg>
							</g:Label>
						</g:Cell>
						<g:Cell width="" height="100%">
							<g:Label ui:field="street2" height="100%" />
						</g:Cell>
					</g:HorizontalPanel>
				</g:HTMLPanel>
			</g:Cell>
			<g:Cell height="30px">
				<g:HTMLPanel styleName="{currentDetails.currentDetailsSpacing}">
					<g:HorizontalPanel width="100%">
						<g:Cell width="150px">
							<g:Label styleName="{content.contactDetailsLabel}">
								<ui:msg key="city" description="City/Town Name">City/Town</ui:msg>
							</g:Label>
						</g:Cell>
						<g:Cell width="">
							<g:Label ui:field="city" />
						</g:Cell>
					</g:HorizontalPanel>
				</g:HTMLPanel>
			</g:Cell>
			<g:Cell height="30px">
				<g:HTMLPanel styleName="{currentDetails.currentDetailsSpacing}">
					<g:HorizontalPanel width="100%">
						<g:Cell width="150px">
							<g:Label styleName="{content.contactDetailsLabel}">
								<ui:msg key="state" description="State Name">State</ui:msg>
							</g:Label>
						</g:Cell>
						<g:Cell width="">
							<g:Label ui:field="state" />
						</g:Cell>
					</g:HorizontalPanel>
				</g:HTMLPanel>
			</g:Cell>
			<g:Cell height="30px">
				<g:HTMLPanel styleName="{currentDetails.currentDetailsSpacing}">
					<g:HorizontalPanel width="100%">
						<g:Cell width="150px">
							<g:Label styleName="{content.contactDetailsLabel}">
								<ui:msg key="zipCode" description="Postal Zip Code">Postal Code</ui:msg>
							</g:Label>
						</g:Cell>
						<g:Cell width="">
							<g:Label ui:field="zipCode" />
						</g:Cell>
					</g:HorizontalPanel>
				</g:HTMLPanel>
			</g:Cell>
			<g:Cell height="30px">
				<g:HTMLPanel styleName="{currentDetails.currentDetailsSpacing}">
					<g:HorizontalPanel width="100%">
						<g:Cell width="150px">
							<g:Label styleName="{content.contactDetailsLabel}">
								<ui:msg key="country" description="Country Name">Country</ui:msg>
							</g:Label>
						</g:Cell>
						<g:Cell width="">
							<g:Label ui:field="country" />
						</g:Cell>
					</g:HorizontalPanel>
				</g:HTMLPanel>
			</g:Cell>
		</g:VerticalPanel>
	</g:HTMLPanel>
</ui:UiBinder> 